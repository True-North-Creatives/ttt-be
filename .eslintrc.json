{
    "env": {
        "node": true,
        "jest": false
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "warn",
        "func-names": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "no-param-reassign": ["error", { "props": false }],
        "indent": "off",
        "comma-dangle": "off"
    }
}
